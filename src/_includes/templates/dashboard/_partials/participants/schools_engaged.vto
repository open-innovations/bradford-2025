{{ comp dashboard.Cell {
	title: metrics.participants_schools_engaged.title,
	flash: metrics.participants_schools_engaged.flash,
	classes: 'participants'
} }}
	{{ comp.BigNumber({
		number: manual.participants.schools ,
		suffix: '',
	}) }}
